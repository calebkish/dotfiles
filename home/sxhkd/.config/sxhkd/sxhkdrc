button6
	xte 'key XF86AudioLowerVolume'

button7
	xte 'key XF86AudioRaiseVolume'

XF86AudioLowerVolume
	amixer -D pulse set Master 3%-
	#volctl PCH Master 3%-

XF86AudioRaiseVolume
	amixer -D pulse set Master 3%+
	#volctl PCH Master 3%+

super + bracketleft
	brightctl DP-2 down 0.05

super + bracketright
	brightctl DP-2 up 0.05
